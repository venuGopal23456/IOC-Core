<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    <bean id="adrs" class="Test.Address" >
        <property name="city" value="arni"/>
        <property name="state" value="123"/>
        <property name="zipcode" value="455"/>

    </bean>
    <bean id="t" class="Test.test"  init-method="myinit" destroy-method="mydestroy">
        <property name="ename" value="venu"/>
        <property name="eid" value="123"/>
        <constructor-arg ref="adrs"/>

    </bean>
</beans>
